function DelAbate(id)
{
    if ( confirm("注意:该项操作将会遍历所有文章和所有评论,请尽量在服务器不繁忙时使用!!是否继续？"))
        {
            window.location.href = "admin/reviewmanage.asp?action=delabate&newsid=" + id
        }
}
function DelAll(id)
{
    if ( confirm("警告!执行该操作将会删除全部文章的所有评论!!!是否继续？"))
        {
            window.location.href = "admin/reviewmanage.asp?action=delall&newsid=" + id
        }
}
function DelOne(rid,id)
{
    if ( confirm("是否删除本条评论？"))
        {
            window.location.href = "admin/reviewmanage.asp?action=delone&newsid=" + id + "&reviewid=" + rid
        }
}
function DelAbout(id)
{
    if ( confirm("警告!执行该操作将会删除与本文章相关的评论!!!是否继续？"))
        {
            window.location.href = "admin/reviewmanage.asp?action=delabout&newsid=" + id
        }
}
